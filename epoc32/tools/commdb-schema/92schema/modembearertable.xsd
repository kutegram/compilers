                        <xsd:element minOccurs="0" maxOccurs="0" name="ProtocolLAPD" type="Text"/>
                        <xsd:element minOccurs="0" maxOccurs="0" name="ProtocolALT" type="Text"/>
                        <xsd:element minOccurs="0" maxOccurs="0" name="ProtocolALTCELLULAR" type="Text"/>
